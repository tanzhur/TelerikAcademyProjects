@model ManagementSystem.Models.Plan

@{
    ViewBag.Title = "Delete";
}

<h2>Are you sure you want to delete @Html.DisplayFor(model => model.Title)?</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="form-actions no-color">
        @Html.ActionLink("Back", "Details", new { id = Model.Id }, new { @class = "btn btn-small" })
        <input type="submit" value="Delete" class="btn btn-small btn-warning" />
    </div>
}
